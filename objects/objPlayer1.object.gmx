///Movement

//Gamepad controls
if(joystick_exists(1))
{    
    vspeed = joystick_ypos(1)*speedmultiplier;
    hspeed = joystick_xpos(1)*speedmultiplier; 
} else {
    vspeed = 0;
    hspeed = 0;
    if (keyboard_check(vk_up)) {
        vspeed = -speedmultiplier;
    } else if (keyboard_check(vk_down)) {
        vspeed = speedmultiplier;
    }
    if (keyboard_check(vk_left)) {
        hspeed = -speedmultiplier;
    } else if (keyboard_check(vk_right)) {
        hspeed = speedmultiplier;
    }
}

//Image speed control
if speed != 0
{
    image_speed = 0.5;
    
    //Image direction controls
    if direction &lt; 45 or direction &gt; 315
    {
        //right
        if !(image_index &gt; 3 and image_index &lt; 5)
        {
            image_index = 3;
        }
    }
    else if direction &lt; 135 and direction &gt; 45
    {
        //up
        if !(image_index &gt; 0 and image_index &lt; 3)
        {
            image_index = 0;
        }
    }
    else if direction &lt; 225 and direction &gt; 135
    {
        //left
        if !(image_index &gt; 9 and image_index &lt; 11)
        {
            image_index = 9;
        }
    }
    else if direction &lt; 315 and direction &gt; 225
    {
        //down
        if !(image_index &gt; 7 and image_index &lt; 9)
        {
            image_index = 7;
        }
    }
}
else
{
    image_speed = 0;
}

